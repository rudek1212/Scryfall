<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Scryfall API services</Title>
    <PackageId>ScryfallWrapper.Client</PackageId>
    <Authors>Rudek</Authors>
    <Description>Simple Scryfall api wrapper.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>scryfall, scryfall-api</PackageTags>
    <RepositoryUrl>https://github.com/rudek1212/Scryfall</RepositoryUrl>
    <PackageProjectUrl>https://github.com/rudek1212/Scryfall</PackageProjectUrl>
	<Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Configuration\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="7.0.4" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="7.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
